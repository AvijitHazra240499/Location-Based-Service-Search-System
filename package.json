{
  "name": "location-service-search",
  "version": "1.0.0",
  "description": "Location-based service search system with Express, TypeScript, MySQL, and Swagger",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "seed": "ts-node src/database/seed.ts",
    "test": "jest --runInBand"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "typeorm": "^0.3.19",
    "reflect-metadata": "^0.1.13",
    "mysql2": "^3.11.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.3",
    "@types/supertest": "^2.0.12",
    "@types/node": "^20.3.1",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/dotenv": "^8.2.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "testMatch": ["**/tests/**/*.test.(ts|js)"]
  }
}
